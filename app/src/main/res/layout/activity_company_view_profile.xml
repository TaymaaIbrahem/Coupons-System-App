<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".CompanyViewProfileActivity">

    <ImageButton
        android:id="@+id/fragCompViewProfile_btnBack"
        android:layout_width="63dp"
        android:layout_height="51dp"
        android:backgroundTint="#F6F2F2"
        android:scaleType="fitCenter"
        app:layout_constraintBottom_toTopOf="@+id/fragCompViewProfile_tvComID"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.045"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.64"
        app:srcCompat="@drawable/back" />

    <TextView
        android:id="@+id/fragCompViewProfile_tvComID"
        android:layout_width="140dp"
        android:layout_height="44dp"
        android:layout_marginTop="76dp"
        android:background="@drawable/border"
        android:gravity="center|start"
        android:text="companyID"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.114"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/fragCompViewProfile_tvComName"
        android:layout_width="211dp"
        android:layout_height="46dp"
        android:layout_marginTop="44dp"
        android:background="@drawable/border"
        android:gravity="center|start"
        android:text="name"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="@+id/fragCompViewProfile_tvComID"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/fragCompViewProfile_tvComID"
        app:layout_constraintTop_toBottomOf="@+id/fragCompViewProfile_tvComID" />

    <TextView
        android:id="@+id/fragCompViewProfile_tvComEmail"
        android:layout_width="353dp"
        android:layout_height="45dp"
        android:layout_marginTop="44dp"
        android:background="@drawable/border"
        android:gravity="center|start"
        android:text="email"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="@+id/fragCompViewProfile_tvComName"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/fragCompViewProfile_tvComName"
        app:layout_constraintTop_toBottomOf="@+id/fragCompViewProfile_tvComName" />

    <TextView
        android:id="@+id/fragCompViewProfile_tvComPassword"
        android:layout_width="243dp"
        android:layout_height="43dp"
        android:layout_marginTop="44dp"
        android:background="@drawable/border"
        android:gravity="center|start"
        android:text="password"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="@+id/fragCompViewProfile_tvComEmail"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/fragCompViewProfile_tvComEmail"
        app:layout_constraintTop_toBottomOf="@+id/fragCompViewProfile_tvComEmail" />

</androidx.constraintlayout.widget.ConstraintLayout>