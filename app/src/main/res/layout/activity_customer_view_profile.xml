<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
xmlns:app="http://schemas.android.com/apk/res-auto"
xmlns:tools="http://schemas.android.com/tools"
android:layout_width="match_parent"
android:layout_height="match_parent"
tools:context=".CustomerViewProfileActivity">

<TextView
    android:id="@+id/fragCustViewProfile_tvCustID"
    android:layout_width="140dp"
    android:layout_height="44dp"
    android:layout_marginTop="76dp"
    android:background="@drawable/border"
    android:gravity="center|start"
    android:text="customerID"
    android:textSize="20sp"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintHorizontal_bias="0.114"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toTopOf="parent" />

<TextView
    android:id="@+id/fragCustViewProfile_tvCustName"
    android:layout_width="211dp"
    android:layout_height="46dp"
    android:layout_marginTop="44dp"
    android:background="@drawable/border"
    android:gravity="center|start"
    android:text="name"
    android:textSize="20sp"
    app:layout_constraintEnd_toEndOf="@+id/fragCustViewProfile_tvCustID"
    app:layout_constraintHorizontal_bias="0.0"
    app:layout_constraintStart_toStartOf="@+id/fragCustViewProfile_tvCustID"
    app:layout_constraintTop_toBottomOf="@+id/fragCustViewProfile_tvCustID" />

<TextView
    android:id="@+id/fragCustViewProfile_tvCustEmail"
    android:layout_width="353dp"
    android:layout_height="45dp"
    android:layout_marginTop="44dp"
    android:background="@drawable/border"
    android:gravity="center|start"
    android:text="email"
    android:textSize="20sp"
    app:layout_constraintEnd_toEndOf="@+id/fragCustViewProfile_tvCustName"
    app:layout_constraintHorizontal_bias="0.0"
    app:layout_constraintStart_toStartOf="@+id/fragCustViewProfile_tvCustName"
    app:layout_constraintTop_toBottomOf="@+id/fragCustViewProfile_tvCustName" />

<TextView
    android:id="@+id/fragCustViewProfile_tvCustPassword"
    android:layout_width="243dp"
    android:layout_height="43dp"
    android:layout_marginTop="44dp"
    android:background="@drawable/border"
    android:gravity="center|start"
    android:text="password"
    android:textSize="20sp"
    app:layout_constraintEnd_toEndOf="@+id/fragCustViewProfile_tvCustEmail"
    app:layout_constraintHorizontal_bias="0.0"
    app:layout_constraintStart_toStartOf="@+id/fragCustViewProfile_tvCustEmail"
    app:layout_constraintTop_toBottomOf="@+id/fragCustViewProfile_tvCustEmail" />

</androidx.constraintlayout.widget.ConstraintLayout>